{
  "name": "zelda-editor-vue",
  "version": "0.7.0",
  "private": true,
  "scripts": {
    "clean": "rimraf build dist doc coverage",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest --coverage src/",
    "lint": "eslint --ext .ts src/",
    "tsc": "tsc --noEmit",
    "build-electron": "cross-env NODE_ENV=dev webpack --config webpack/webpack-desktop.config.js",
    "dist": "electron-builder",
    "doc": "typedoc --mode file --target ES5 --out doc/ ./src",
    "pack": "electron-builder --dir"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "debounce": "2.2.0",
    "gtp": "2.0.5",
    "jshighlight": "0.3.3",
    "uuid": "11.0.2",
    "vue": "3.5.12",
    "vuetify": "3.7.3",
    "vuex": "4.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "8.13.0",
    "@typescript-eslint/parser": "8.13.0",
    "@vitejs/plugin-vue": "5.1.4",
    "@vitest/coverage-v8": "2.1.4",
    "coveralls": "3.1.0",
    "eslint": "8.57.1",
    "less": "3.11.3",
    "rimraf": "6.0.1",
    "sass": "1.26.8",
    "typedoc": "0.17.7",
    "typescript": "5.6.3",
    "vite": "5.4.10",
    "vite-plugin-vuetify": "2.0.4",
    "vitest": "2.1.4",
    "vue-eslint-parser": "9.4.3"
  }
}
