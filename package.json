{
  "name": "zelda-editor-vue",
  "version": "0.7.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf build dist doc coverage",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest --coverage src/",
    "lint": "eslint --fix src/",
    "tsc": "tsc --noEmit",
    "build-electron": "cross-env NODE_ENV=dev webpack --config webpack/webpack-desktop.config.js",
    "dist": "electron-builder",
    "doc": "typedoc --excludeExternals --out doc/ --name zelda --exclude **/*.spec.ts src/**/*.ts",
    "pack": "electron-builder --dir"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "debounce": "2.2.0",
    "gtp": "3.0.1",
    "jshighlight": "0.3.3",
    "uuid": "11.1.0",
    "vue": "3.5.17",
    "vuetify": "3.9.2",
    "vuex": "4.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "8.40.0",
    "@vitejs/plugin-vue": "6.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "coveralls": "3.1.1",
    "eslint": "9.31.0",
    "rimraf": "6.0.1",
    "sass-embedded": "1.89.2",
    "typedoc": "0.28.7",
    "typescript": "5.8.3",
    "typescript-eslint": "8.40.0",
    "vite": "7.0.5",
    "vite-plugin-vuetify": "2.1.1",
    "vitest": "3.2.4",
    "vue-eslint-parser": "10.2.0"
  }
}
