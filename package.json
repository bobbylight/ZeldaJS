{
  "name": "zelda-editor-vue",
  "version": "0.7.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf build dist doc coverage",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest --coverage src/",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "tsc": "tsc --noEmit",
    "build-electron": "cross-env NODE_ENV=dev webpack --config webpack/webpack-desktop.config.js",
    "dist": "electron-builder",
    "doc": "typedoc --excludeExternals --out doc/ --name zelda --exclude **/*.test.ts src/**/*.ts",
    "pack": "electron-builder --dir"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "debounce": "3.0.0",
    "gtp": "4.1.0",
    "jshighlight": "0.3.3",
    "uuid": "13.0.0",
    "vue": "3.5.24",
    "vuetify": "3.10.11",
    "vuex": "4.1.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "5.6.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/user-event": "14.6.1",
    "@testing-library/vue": "8.1.0",
    "@typescript-eslint/parser": "8.47.0",
    "@vitejs/plugin-vue": "6.0.2",
    "@vitest/coverage-v8": "4.0.13",
    "canvas": "3.2.0",
    "eslint": "9.39.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-vue": "10.6.0",
    "jsdom": "27.2.0",
    "resize-observer-polyfill": "1.5.1",
    "rimraf": "6.1.2",
    "sass-embedded": "1.93.3",
    "typedoc": "0.28.14",
    "typescript": "5.9.3",
    "typescript-eslint": "8.47.0",
    "vite": "7.2.4",
    "vite-plugin-vuetify": "2.1.2",
    "vitest": "4.0.13",
    "vue-eslint-parser": "10.2.0"
  }
}
