{
  "name": "zelda-editor-vue",
  "version": "0.7.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf build dist doc coverage",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest --coverage src/",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "tsc": "tsc --noEmit",
    "build-electron": "cross-env NODE_ENV=dev webpack --config webpack/webpack-desktop.config.js",
    "dist": "electron-builder",
    "doc": "typedoc --excludeExternals --out doc/ --name zelda --exclude **/*.spec.ts src/**/*.ts",
    "pack": "electron-builder --dir"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "debounce": "2.2.0",
    "gtp": "3.0.1",
    "jshighlight": "0.3.3",
    "uuid": "11.1.0",
    "vue": "3.5.19",
    "vuetify": "3.9.5",
    "vuex": "4.1.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "5.2.3",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/user-event": "14.6.1",
    "@testing-library/vue": "8.1.0",
    "@typescript-eslint/parser": "8.40.0",
    "@vitejs/plugin-vue": "6.0.1",
    "@vitest/coverage-v8": "3.2.4",
    "canvas": "3.2.0",
    "coveralls": "3.1.1",
    "eslint": "9.34.0",
    "eslint-plugin-vue": "10.4.0",
    "jsdom": "26.1.0",
    "rimraf": "6.0.1",
    "sass-embedded": "1.90.0",
    "typedoc": "0.28.10",
    "typescript": "5.9.2",
    "typescript-eslint": "8.40.0",
    "vite": "7.1.11",
    "vite-plugin-vuetify": "2.1.2",
    "vitest": "3.2.4",
    "vue-eslint-parser": "10.2.0"
  }
}
