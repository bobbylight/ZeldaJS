{
  "name": "zelda-editor-vue",
  "version": "0.6.0",
  "private": true,
  "scripts": {
    "clean": "rimraf build dist doc coverage",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit src/*.spec.ts",
    "lint": "vue-cli-service lint",
    "travis-ci": "cross-env NODE_ENV=test nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "doc": "typedoc --mode file --target ES5 --out doc/ ./src",
    "build-electron": "cross-env NODE_ENV=dev webpack --config webpack/webpack-desktop.config.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "dependencies": {
    "debounce": "1.2.0",
    "gtp": "2.0.4",
    "jshighlight": "0.3.2",
    "uuid": "3.3.3",
    "vue": "2.6.11",
    "vue-property-decorator": "8.3.0",
    "vuetify": "2.2.3",
    "vuex": "3.1.2"
  },
  "devDependencies": {
    "@types/chai": "4.2.7",
    "@types/debounce": "1.2.0",
    "@types/mocha": "5.2.7",
    "@vue/cli-plugin-typescript": "4.1.2",
    "@vue/cli-plugin-unit-mocha": "4.1.2",
    "@vue/cli-plugin-vuex": "4.1.2",
    "@vue/cli-service": "4.1.2",
    "@vue/test-utils": "1.0.0-beta.29",
    "chai": "4.2.0",
    "less": "3.10.3",
    "less-loader": "5.0.0",
    "rimraf": "3.0.0",
    "sass": "1.24.4",
    "sass-loader": "8.0.1",
    "typedoc": "0.15.8",
    "typescript": "3.7.4",
    "vue-class-component": "7.2.0",
    "vue-cli-plugin-vuetify": "2.0.3",
    "vue-template-compiler": "2.6.11",
    "vuetify-loader": "1.4.3"
  }
}
